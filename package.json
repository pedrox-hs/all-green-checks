{
  "name": "all-green-checks",
  "version": "0.1.0",
  "description": "GitHub action to verify all checks",
  "main": "dist/index.js",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "build": "tsc",
    "build:ncc": "ncc build src/index.ts --no-source-map-register -m -o dist",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest tests/**/*.test.*",
    "test:coverage": "jest --collect-coverage --coverage-provider=v8 tests/**/*.test.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrox-hs/all-green-checks.git"
  },
  "keywords": [
    "github-actions"
  ],
  "author": "Pedro Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedrox-hs/all-green-checks/issues"
  },
  "homepage": "https://github.com/pedrox-hs/all-green-checks#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^10.1.2",
    "@octokit/webhooks-types": "^7.3.1",
    "@types/node": "^20.8.10",
    "@types/node-fetch": "^2.6.8",
    "nautilustar-debug": "^1.1.0",
    "node-fetch": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vercel/ncc": "^0.38.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nock": "^13.3.8",
    "standard": "^17.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
